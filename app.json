{
  "expo": {
    "name": "Eaton-LLC-MobileApp",
    "slug": "Eaton-LLC-MobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "eatonllcmobileapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eatonllc.partnerapp"
    },
    "android": {
      "package": "com.eatonllc.partnerapp",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1e9bc6a7-8cee-42e2-ad10-9721dd503c78"
      }
    },
    "owner": "eaton26"
  }
}
